<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
   <servlet>
       <servlet-name>StatrServlet</servlet-name>
       <servlet-class>main.java.Servlets.GetIndexPageServlet</servlet-class>
   </servlet>
    <servlet-mapping>
        <servlet-name>StatrServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CategoriesServlet</servlet-name>
        <servlet-class>main.java.Servlets.MongoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CategoriesServlet</servlet-name>
        <url-pattern>/catlist</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>testservlet</servlet-name>
        <servlet-class>main.java.Servlets.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testservlet</servlet-name>
        <url-pattern>/connection</url-pattern>
    </servlet-mapping>

</web-app>